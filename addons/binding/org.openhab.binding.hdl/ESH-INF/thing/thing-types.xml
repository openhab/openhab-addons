	<channel-type id="fhmode">
		<item-type>String</item-type>
		<label>Floor Heating Mode</label>
		<description>DLP Floor Heating Mode Setting</description>
		<tags>
			<tag>heating</tag>
		</tags>
		<state pattern="%s">
			<options>
				<option value="Normal">Normal</option>
				<option value="Day">Day</option>
				<option value="Night">Night</option>
				<option value="Away">Away</option>
				<option value="Timer">Timer</option>
			</options>
		</state>
	</channel-type>

	<channel-type id="acmode">
		<item-type>String</item-type>
		<label>AC Mode</label>
		<description>DLP AC Mode Setting</description>
		<tags>
			<tag>heating</tag>
		</tags>
		<state pattern="%s">
			<options>
				<option value="COOLING">COOLING</option>
				<option value="HEATING">HEATING</option>
				<option value="FAN">FAN</option>
				<option value="AUTO">AUTO</option>
				<option value="DEHUMIDFY">DEHUMIDFY</option>
			</options>
		</state>
	</channel-type>

	<channel-type id="acfanspeed">
		<item-type>String</item-type>
		<label>AC Fan Speed</label>
		<description>DLP AC Fan Speed Setting</description>
		<tags>
			<tag>heating</tag>
		</tags>
		<state pattern="%s">
			<options>
				<option value="AUTO">AUTO</option>
				<option value="HIGH">HIGH</option>
				<option value="MEDIUM">MEDIUM</option>
				<option value="LOW">LOW</option>                
			</options>
		</state>
	</channel-type>

