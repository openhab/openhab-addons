{
    "id": "waterPressureMeasurement",
    "version": 1,
    "status": "proposed",
    "name": "Water Pressure Measurement",
    "ephemeral": false,
    "attributes": {
        "pressureAlarm": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "string",
                        "enum": [
                            "normal",
                            "low",
                            "high"
                        ]
                    }
                },
                "additionalProperties": false,
                "required": [
                    "value"
                ]
            },
            "enumCommands": []
        },
        "pressure": {
            "schema": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "number",
                        "minimum": 0
                    },
                    "unit": {
                        "type": "string",
                        "enum": [
                            "psi",
                            "kPa",
                            "bar"
                        ],
                        "default": "psi"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "value"
                ]
            },
            "enumCommands": []
        }
    },
    "commands": []
}