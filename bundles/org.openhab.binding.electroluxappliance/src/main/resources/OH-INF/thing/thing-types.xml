		<label>Time To End</label>
		<description>The time remaining until the program will end.</description>
		<tags>
			<tag>Measurement</tag>
			<tag>Progress</tag>
		</tags>
		<state readOnly="true" pattern="%d %unit%"/>
	</channel-type>

