		<tags>
			<tag>Status</tag>
			<tag>LockState</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>

	<channel-type id="door-state">
		<item-type>Contact</item-type>
		<label>Door State</label>
		<description>The door status Open/Closed.</description>
		<tags>
			<tag>Status</tag>
			<tag>OpenState</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>

