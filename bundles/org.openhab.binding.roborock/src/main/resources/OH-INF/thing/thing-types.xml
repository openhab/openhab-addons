				<option value="34">Detaching the mop</option>
				<option value="100">Full</option>
				<option value="101">Device Offline</option>
				<option value="103">Locked</option>
				<option value="202">Air drying stopping</option>
				<option value="6301">Robot status mopping</option>
				<option value="6302">Clean mop cleaning</option>
				<option value="6303">Clean mop mopping</option>
				<option value="6304">Segment mopping</option>
				<option value="6305">Segment clean mop cleaning</option>
				<option value="6306">Segment clean mop mopping</option>
				<option value="6307">Zoned mopping</option>
				<option value="6308">Zoned clean mop cleaning</option>
				<option value="6309">Zoned clean mop mopping</option>
				<option value="6310">Back to dock washing duster</option>
			</options>
