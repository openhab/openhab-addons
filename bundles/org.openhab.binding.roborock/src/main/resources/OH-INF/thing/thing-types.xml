		<tags>
			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>
	<channel-type id="lock-status">
		<item-type>Switch</item-type>
		<label>Lock Status</label>
		<tags>
			<tag>Status</tag>
			<tag>LockState</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>
	<channel-type id="mop-forbidden-enable">
		<item-type>Switch</item-type>
		<label>Mop Forbidden</label>
		<tags>
			<tag>Status</tag>
			<tag>Enabled</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>
	<channel-type id="is-locating">
		<item-type>Switch</item-type>
		<label>Robot Locating</label>
		<description>1 = Robot is locating it's position, 0 = robot is not locating it's position</description>
		<tags>
			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>
	<channel-type id="dock-state-id">
		<item-type>Number</item-type>
		<label>Dock State ID</label>
		<tags>
			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
		<state readOnly="true">
			<options>
				<option value="0">OK</option>
				<option value="34">Suction Error</option>
				<option value="38">Error fresh water tank</option>
				<option value="39">Error dirty water tank</option>
				<option value="42">Maintenance brush jammed</option>
				<option value="44">Dirty tank latch open</option>
				<option value="46">Missing dust container/dust bag</option>
				<option value="53">Cleaning tank full or blocked</option>
			</options>
		</state>
	</channel-type>
	<channel-type id="mop-drying-time">
		<item-type>Number:Time</item-type>
