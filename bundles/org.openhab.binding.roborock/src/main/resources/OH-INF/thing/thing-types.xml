		<tags>
			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
		<state pattern="%.0f" readOnly="true"/>
	</channel-type>

	<!-- Dynamic Status Channels -->
	<channel-type id="water-box-mode">
		<item-type>Number</item-type>
		<label>Water Box Mode</label>
		<tags>
			<tag>Control</tag>
			<tag>Mode</tag>
		</tags>
		<state min="200" max="204" step="1" pattern="%.0f%%" readOnly="false">
			<options>
				<option value="200">Off</option>
				<option value="201">Low</option>
				<option value="202">Medium</option>
				<option value="203">High</option>
				<option value="204">Customised</option>
			</options>
		</state>
	</channel-type>
	<channel-type id="mop-mode">
		<item-type>Number</item-type>
		<label>Mop Mode</label>
		<tags>
			<tag>Control</tag>
			<tag>Mode</tag>
		</tags>
		<state min="300" max="303" step="1" pattern="%.0f%%" readOnly="false">
			<options>
				<option value="300">Standard</option>
				<option value="301">Deep</option>
				<option value="303">Deep+</option>
			</options>
		</state>
	</channel-type>
