				<option value="8">Charging</option>
				<option value="9">Charging Error</option>
				<option value="10">Paused</option>
				<option value="11">Spot cleaning</option>
				<option value="12">In Error</option>
				<option value="13">Shutting Down</option>
				<option value="14">Updating</option>
				<option value="15">Docking</option>
				<option value="16">Go To</option>
				<option value="17">Zone Clean</option>
				<option value="18">Room Clean</option>
				<option value="22">Emptying the bin</option>
				<option value="23">Washing the mop</option>
				<option value="25">Washing the mop</option>
				<option value="26">Going to wash the mop</option>
				<option value="28">In call</option>
				<option value="29">Mapping</option>
				<option value="30">Egg attack</option>
				<option value="32">Patrol</option>
