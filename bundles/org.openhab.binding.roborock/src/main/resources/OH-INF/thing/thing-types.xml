			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
		<state readOnly="true"/>
	</channel-type>
	<channel-type id="error-id">
		<item-type>Number</item-type>
		<label>Error ID</label>
		<tags>
			<tag>Status</tag>
			<tag>Info</tag>
		</tags>
